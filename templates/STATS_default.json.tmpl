{% if data['key'].startswith('CPU_STATS:') %}
{
  "v":
    [
      {%- for value in data['values'] %}
        {{value}}{{", " if not loop.last else ""}}
      {%- endfor %}
    ],
  "t": 
    [
      {%- for timestamp in data['timestamps'] %}
        {{timestamp}}{{", " if not loop.last else ""}}
      {%- endfor %}
    ]
}
{% endif %}